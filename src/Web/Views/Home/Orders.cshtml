@model List<OrderViewModel>
@{
    ViewData["Title"] = "My Orders Page";
}

<h2>My Orders</h2>

<table class="table">
    <thead>
        <tr>
            <th>Order</th>
            <th>Order Date</th>
            <th>Order Items</th>
        </tr>
    </thead>
    <tbody>
        @for (int i = 0; i < Model.Count; i++)
        {
            <tr>
                <td>@(i+1)</td>
                <td>@Model[i].OrderDate.ToString()</td>
                <td>@(i+1)</td>
            </tr>
        }
    </tbody>
</table>